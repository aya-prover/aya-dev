prim I : ISet
prim Cof : Set
prim Partial (φ : Cof) (A : Type) : Set
module cof-operators {
  prim cofAnd (φ ψ : Cof) : Cof
  prim cofOr (φ ψ : Cof) : Cof
  prim cofEq (i j : I) : Cof
}
public open cof-operators using
  ( cofAnd as infix ∧f
//  , cofOr as infix ∨f looser ∧f
